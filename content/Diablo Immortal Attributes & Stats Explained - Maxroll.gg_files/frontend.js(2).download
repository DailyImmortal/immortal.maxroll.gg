jQuery(function($){function checkNavigation(parent){const isMobile=window.matchMedia('(max-width: 768px)').matches;if(parent.height()<=825&&!isMobile){parent.find('.navigation.bottom').hide();}}
function showContent(contentId,el){const wrapper=el.parents('.nested-content-wrapper');const contentWrapper=wrapper.find(`div[data-content-id=${ contentId }]`);if(contentWrapper.length>0){contentWrapper.siblings('.nested-content-content-wrapper').hide();contentWrapper.show();checkNavigation(contentWrapper);}}
$('.nested-content-wrapper').each(function(){const wrapper=$(this);const hasInnerTabs=wrapper.find('.advgb-tabs-wrapper');const title=wrapper.find('.select-box__title > i');checkNavigation(wrapper);if(title.length>0&&title.html()===''){title.parents('.select-box__current').addClass('no-title');}
if(hasInnerTabs.length>0){const innerTabsClasses=hasInnerTabs.attr('class').split(' ');innerTabsClasses[innerTabsClasses.length-1]='nested';hasInnerTabs.attr('class',innerTabsClasses.join(' '));}
wrapper.find('.navigation img').click(function(){if(!$(this).hasClass('disabled')){$('html, body').animate({scrollTop:wrapper.offset().top-120},'fast');const extractedId=$(this).parents('.nested-content-content-wrapper').attr('data-content-id');const[name,uniqueId,index]=extractedId.split('_');const numericIndex=parseInt(index);const triggerId=$(this).hasClass('left')?numericIndex-1:numericIndex+1;$(`#${ name }_${ uniqueId }_${ triggerId }`).click();}});});$(' .nested-content-tabs li a').click(function(){const parent=$(this).parent()
const contentId=parent.attr('data-tab-id');parent.siblings().removeClass('active');parent.addClass('active');showContent(contentId,$(this));});$('.nested-content-selector .select-box__input').change(function(){const contentId=$(this).attr('id');showContent(contentId,$(this));});});